{
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": ["react", "jsx-a11y", "import"],
  "rules": {
    "no-underscore-dangle": "off",
    "no-plusplus": "off",
    "no-continue": "warn",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-restricted-syntax": "warn",
    "no-param-reassign": [
      2,
      {
        "props": false
      }
    ]
  }
}
